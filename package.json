{
  "name": "r3-w1-boilerplate",
  "version": "0.0.1",
  "description": "R3(React + Reflux + REST api) & W(Websocket) Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf build/* & node webpack.dev.server.js & npm run server",
    "server": "nodemon server",
    "build": "rm -rf build/* && ./node_modules/.bin/webpack",
    "dist": "rm -rf dist/* && ./node_modules/.bin/webpack -p --config webpack.production.config.js",
    "lint": "./node_modules/.bin/eslint src/",
    "test": "./node_modules/.bin/karma start ./test/karma.conf.js --reporters mocha,coverage",
    "test:watch": "./node_modules/.bin/karma start ./test/karma.conf.js --no-single-run --reporters mocha"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-2": "^6.1.2",
    "babel-runtime": "^6.2.0",
    "body-parser": "^1.14.2",
    "css-loader": "^0.23.0",
    "eslint": "^1.8.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.7.1",
    "express": "^4.13.3",
    "fs": "0.0.2",
    "generic-pool": "^2.3.0",
    "history": "^2.0.0-rc2",
    "html-webpack-plugin": "^1.7.0",
    "http": "0.0.0",
    "immutable": "^3.7.6",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jquery": "^2.1.4",
    "mysql": "^2.10.0",
    "node-sass": "^3.4.1",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.8.1",
    "page": "^1.6.4",
    "path": "^0.12.7",
    "postcss-loader": "^0.8.0",
    "react-bootstrap": "^0.28.1",
    "react-hot-loader": "^1.3.0",
    "react-router": "1.0.0-rc1",
    "react-router-component": "^0.29.0",
    "react-socket": "^0.1.4",
    "reflux": "0.2.7",
    "sass-loader": "^3.1.1",
    "socket.io": "^1.4.4",
    "style-loader": "^0.13.0",
    "superagent": "^1.6.1",
    "superagent-jsonapify": "^1.4.1",
    "url": "^0.11.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "author": "trustyoo86",
  "license": "MIT"
}
